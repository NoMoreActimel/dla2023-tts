{
    "name": "ljspeech_fastspeech2_test",
    "n_gpu": 1,
    "preprocessing": {
      "spectrogram": {
        "type": "MelSpectrogram",
        "args": {
        }
      },
      "use_pitch_spectrogram": false,
      "pitch_energy_normalization": true,
      "sr": 22050,
      "max_wav_value": 32768.0,
      "hop_size": 256,
      "val_size": 0.1,
      "random_state": 42,
      "TacotronSTFT": {
        "filter_length": 1024,
        "hop_length": 256,
        "win_length": 1024
      },
      "mfa": {
        "perform": false,
        "download": true,
        "bin_path": "",
        "pretrained_model_path": ""
      },
      "log_spec": true
    },
    "augmentations": {
      "wave": [],
      "spectrogram": []
    },
    "arch": {
      "type": "FastSpeech2",
      "args": {}
    },
    "data": {
      "train": {
        "batch_size": 10,
        "num_workers": 5,
        "datasets": [
          {
            "type": "LJspeechFastSpeech2Dataset",
            "args": {
              "part": "",
              "raw_data_dir": "/content/dla2023-tts/data/datasets/ljspeech/raw_data",
              "data_dir": "/content/dla2023-tts/data/datasets/ljspeech/data"
            }
          }
        ]
      },
      "val": {
        "batch_size": 10,
        "num_workers": 5,
        "datasets": [
          {
            "type": "LJspeechFastSpeech2Dataset",
            "args": {
              "part": "",
              "raw_data_dir": "/content/dla2023-tts/data/datasets/ljspeech/raw_data",
              "data_dir": "/content/dla2023-tts/data/datasets/ljspeech/data"
            }
          }
        ]
      }
    },
    "optimizer": {
      "type": "Adam",
      "args": {
        "lr": 3e-4
      }
    },
    "loss": {
      "type": "SpExPlusLoss",
      "args": {}
    },
    "metrics": [
    ],
    "lr_scheduler": {
      "type": "CosineAnnealingWarmRestarts",
      "args": {
        "eta_min": 3e-5,
        "T_0": 2000
      }
    },
    "trainer": {
      "epochs": 50,
      "save_dir": "saved/",
      "save_period": 5,
      "verbosity": 2,
      "monitor": "min val_loss",
      "early_stop": 100,
      "visualize": "wandb",
      "wandb_project": "ss_project",
      "wandb_run_name": "one_batch_test",
      "len_epoch": 100,
      "grad_norm_clip": 10
    }
  }